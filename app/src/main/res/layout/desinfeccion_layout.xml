            android:gravity="center" />

        <TextView
            android:gravity="center"
            android:text="@string/lbl_conideal_turbidez"
            android:textColor="@color/white"/>
    </TableRow>

    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip"
        android:gravity="center">

        <TextView
            android:text="@string/lbl_metales_cloro"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textColor="@color/white"/>

        <TextView
            android:id="@+id/tv_metales"
            android:text="0"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:gravity="center" />

        <TextView
            android:gravity="center"
            android:text="@string/lbl_conideal_metales"
            android:textColor="@color/white"/>
    </TableRow>

    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip"
        android:gravity="center">

        <TextView
            android:text="@string/lbl_cya"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textColor="@color/white"/>

        <TextView
            android:id="@+id/tv_cya"
            android:text="0"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:gravity="center" />

        <TextView
            android:gravity="center"
            android:text="@string/lbl_conideal_cya"
            android:textColor="@color/white"/>
    </TableRow>





</TableLayout>